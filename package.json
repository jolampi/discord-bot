    "tsc": "tsc"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/lodash": "^4.14.161",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.0.23",
    "@types/ws": "^7.2.6",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^4.1.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "mocha": "^8.1.3",
    "prettier": "^2.1.1",
    "typemoq": "^2.1.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@discordjs/opus": "^0.3.2",
