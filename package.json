    "lodash": "^4.17.20",
    "ytdl-core": "^3.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,jsx,d.ts,ts,tsx}": [
      "eslint --cache --ignore-pattern '!.*' --max-warnings 0"
    ],
    "*.{js,json,jsx,d.ts,ts,tsx,yml}": [
      "prettier --check"
    ]
  }
